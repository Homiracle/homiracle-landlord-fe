      "package": "com.hpcc.homiracle.landlord",
      "blockedPermissions": [
        "android.permission.RECORD_AUDIO"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-localization"
    ],
    "extra": {
      "eas": {
        "projectId": "a98bdd88-eafc-43b8-8e4a-276a61775669"
      }
